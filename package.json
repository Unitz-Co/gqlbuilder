{
  "name": "@unitz/gqlbuilder",
  "version": "1.0.2",
  "description": "GraphQL query builder lib",
  "private": false,
  "main": "dist/gqlbuilder.cjs.js",
  "module": "dist/gqlbuilder.esm.js",
  "browser": "dist/gqlbuilder.umd.js",
  "keywords": [
    "graphql",
    "builder"
  ],
  "homepage": "https://github.com/Unitz-Co/gqlbuilder.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/Unitz-Co/gqlbuilder.git"
  },
  "author": "Hung Tran <hungtran0203@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "watch": "concurrently 'yarn build:watch' 'yarn serve -l tcp://127.0.0.1'",
    "clean": "rimraf ./dist",
    "test": "jest --runInBand",
    "test:u": "jest --runInBand --updateSnapshot",
    "test:watch": "jest --watch --coverage",
    "coverage": "jest --collectCoverageFrom=./**.js --coverage ./"
  },
  "dependencies": {
    "graphql": "^15.5.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.0",
    "@babel/plugin-syntax-dynamic-import": "^7.7.0",
    "@babel/plugin-transform-destructuring": "^7.7.0",
    "@babel/plugin-transform-for-of": "^7.7.0",
    "@babel/plugin-transform-regenerator": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.7.0",
    "@loadable/babel-plugin": "^5.10.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.9.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "babel-plugin-macros": "^3.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "4.1.2",
    "graphql-tag": "^2.11.0",
    "rollup": "^2.30.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-visualizer": "^4.2.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "serve": "11.3.2",
    "jest": "^26.5.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^12.0.0",
    "jest-runner-eslint": "^0.10.0",
    "lint-staged": "^10.4.2",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint": "^7.8.1",
    "eslint-plugin-autofix": "^1.0.2",
    "eslint-plugin-unused-imports": "^1.0.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.14.2"
  }
}
